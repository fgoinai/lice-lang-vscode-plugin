{
    "name": "lice",
    "displayName": "Lice",
    "description": "Lice language VS Code plugin",
    "version": "0.0.1",
    "publisher": "FGO",
    "license": "GPLv3",
    "engines": {
        "vscode": "^1.19.0"
    },
    "keywords": [
        "language",
        "lice",
        "lisp",
        "highlight",
        "syntax"
    ],
    "categories": [
        "Languages"
    ],
    "homepage": "https://github.com/lice-lang/lice-lang-vscode-plugin",
    "repository": {
        "type": "git",
        "url": "https://github.com/lice-lang/lice-lang-vscode-plugin.git"
    },
    "bugs": {
        "email": "fgoinai@gmail.com",
        "url": "https://github.com/lice-lang/lice-lang-vscode-plugin/issues"
    },
    "contributes": {
        "languages": [{
            "id": "lice",
            "aliases": ["Lice", "lice"],
            "extensions": [".lice"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lice",
            "scopeName": "source.lice",
            "path": "./syntaxes/lice.tmLanguage.json"
        }],
        "snippets": [
            {"language": "lice", "path": "./snippets/lice.json"}
        ]
    }
}